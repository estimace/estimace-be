{
  "name": "estimace-be",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --esm src/server.ts",
    "dev": "onchange -i -k './src/**/*.ts' -- yarn start",
    "db:migrate:make": "yarn knex migrate:make",
    "db:migrate:latest": "yarn knex migrate:latest",
    "db:migrate:rollback": "yarn knex migrate:rollback",
    "test": "jest --forceExit",
    "test:detect-leaks": "jest --detectLeaks --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@types/ws": "^8.5.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/validator": "^13.7.17",
    "jest": "^29.6.1",
    "onchange": "^7.1.0",
    "supertest": "^6.3.3",
    "superwstest": "^2.0.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.5"
  }
}
